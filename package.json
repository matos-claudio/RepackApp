{
  "name": "RepackApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native webpack-start --reset-cache",
    "bundle:ios": "react-native webpack-bundle --platform ios --entry-file index.js --dev=false",
    "bundle:android": "react-native webpack-bundle --platform android --entry-file index.js --dev=false",
    "webpack": "webpack -c webpack.config.mjs --env platform=ios --env mode=production",
    "start:remote": "node serve-remote-bundles.js",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.2",
    "metro-react-native-babel-preset": "^0.77.0",
    "react": "19.0.0",
    "react-native": "0.79.2"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/runtime": "^7.25.0",
    "@callstack/repack": "^5.1.0-rc.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.2",
    "@react-native/eslint-config": "0.79.2",
    "@react-native/metro-config": "0.79.2",
    "@react-native/typescript-config": "0.79.2",
    "@rspack/core": "^1.3.4",
    "@swc/helpers": "^0.5.17",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "babel-loader": "^10.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4",
    "webpack-cli": "^6.0.1"
  },
  "engines": {
    "node": ">=18"
  }
}
